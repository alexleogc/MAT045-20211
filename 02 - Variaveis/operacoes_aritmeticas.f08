program operacoes_basicas
    implicit none
    real :: a,b

    print*, 'Digite um valor para a: '
    read*, a
    print*, 'Digite um valor para b: '
    read*, b

    print*,'a+b = ', a+b
    print*,'a-b = ', a-b
    print*,'a*b = ', a*b
    print*,'a/b = ', a/b
    print*,'a**b = ', a**b


end program operacoes_basicas